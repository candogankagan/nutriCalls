{
  "name": "nutriCalls",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "ios15": "react-native run-ios --simulator='iPhone 15 Pro Max'",
    "ios13": "react-native run-ios --simulator='iPhone 15'",
    "iosSE": "react-native run-ios --simulator='iPhone SE (3rd generation)'",
    "assets-load": "sudo npx react-native-asset",
    "podReset": "cd ios && pod cache clean --all && rm -rf Pods && rm -rf Podfile.lock && pod install",
    "buildApk": "cd android && ./gradlew assembleRelease",
    "cleanCache": "npx react-native start --reset-cache",
    "iosClean": "cd ios && if [ -d build ]; then xattr -w com.apple.xcode.CreatedByBuildSystem true build; fi && xcodebuild clean && rm -rf ~/Library/Developer/Xcode/DerivedData",
    "androidClean": "cd android && ./gradlew clean"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/datetimepicker": "^8.4.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "@reduxjs/toolkit": "^2.7.0",
    "lottie-react-native": "^7.2.2",
    "react": "19.0.0",
    "react-native": "0.79.1",
    "react-native-calendars": "^1.1312.1",
    "react-native-chart-kit": "^6.12.0",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0",
    "react-native-svg": "^15.12.0",
    "react-native-vector-icons": "^10.2.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.1",
    "@react-native/eslint-config": "0.79.1",
    "@react-native/metro-config": "0.79.1",
    "@react-native/typescript-config": "0.79.1",
    "@types/i18next": "^13.0.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-i18next": "^8.1.0",
    "@types/react-test-renderer": "^19.0.0",
    "@types/redux-logger": "^3.0.13",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
